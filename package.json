{
  "name": "c4",
  "private": true,
  "packageManager": "pnpm@7.3.0",
  "scripts": {
    "build": "vite build && jszip-cli -c jszip.config.json",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "vite-dev": "vite --port 3333",
    "webxdc-dev": "webxdc-dev run http://localhost:3333/",
    "dev": "concurrently \"pnpm webxdc-dev\" \"pnpm vite-dev\""
  },
  "dependencies": {
    "@brajkowski/connect4-logic": "^1.0.6",
    "connect4-engine": "^1.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "usehooks-ts": "^2.5.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.23.1",
    "@ffflorian/jszip-cli": "^3.1.6",
    "@testing-library/react": "^13.3.0",
    "@types/node": "^17.0.45",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@unocss/reset": "^0.34.1",
    "@vitejs/plugin-react": "^1.3.2",
    "concurrently": "^7.2.2",
    "eruda": "^2.4.1",
    "eslint": "^8.18.0",
    "jsdom": "^19.0.0",
    "typescript": "^4.7.4",
    "unocss": "^0.34.1",
    "unplugin-auto-import": "^0.7.2",
    "vite": "^2.9.12",
    "vitest": "^0.12.10",
    "webxdc-dev": "^0.11.0"
  },
  "eslintConfig": {
    "extends": "@antfu"
  },
  "eslintIgnore": [
    "webxdc.js",
    "webxdc.d.ts"
  ]
}
